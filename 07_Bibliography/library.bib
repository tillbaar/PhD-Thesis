journal = {Nature biotechnology},
keywords = {Algorithms,Alleles,Arabidopsis,Arabidopsis: metabolism,Arabis,Arabis: genetics,Base Pairing,Base Pairing: genetics,Base Sequence,Chromosome Mapping,Crosses, Genetic,Ethyl Methanesulfonate,Flowers,Flowers: genetics,Genes, Plant,Genes, Plant: genetics,Genome, Plant,Genome, Plant: genetics,Molecular Sequence Data,Mutation,Mutation: genetics,Oryza sativa,Oryza sativa: genetics,Plant Proteins,Plant Proteins: genetics,Plant Proteins: metabolism,Reference Standards,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Sequence Deletion},
month = {apr},
