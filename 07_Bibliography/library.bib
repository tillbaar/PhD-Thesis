journal = {Journal of cell science},
keywords = {Chlamydomonas,Chlamydomonas: ultrastructure,Fluorescent Dyes,Microscopy, Electron,Microscopy, Fluorescence,Microscopy, Phase-Contrast,Mitochondria,Mitochondria: ultrastructure,Osmotic Pressure,Rhodamine 123,Rhodamines},
month = {jun},
number = {1985},
pages = {145--53},
pmid = {3905835},
title = {{Changes in the shape of mitochondria following osmotic stress to the unicellular green alga Chlamydomonas reinhardii.}},
