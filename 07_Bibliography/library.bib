keywords = {Anti-Infective Agents,Base Sequence,Computational Biology,Databases, Genetic,Drug Resistance, Microbial,Drug Resistance, Microbial: genetics,Genes, Bacterial,Genome, Bacterial,Internet,User-Computer Interface},
