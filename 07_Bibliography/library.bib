file = {:C$\backslash$:/Users/Admin/Documents/Papers/2011/2011 - Anderberg, Danielson, Johanson.pdf:pdf},
issn = {1471-2148},
journal = {BMC evolutionary biology},
keywords = {Amino Acid Sequence,Aquaporins,Aquaporins: genetics,Chlorophyta,Chlorophyta: genetics,Conserved Sequence,Genome, Plant,Molecular Sequence Data,Phylogeny,Plant Proteins,Plant Proteins: genetics,Plants,Plants: genetics,Sequence Alignment},
month = {jan},
number = {1},
