month = {apr},
number = {6832},
pages = {1091--6},
pmid = {11323671},
title = {{The highly reduced genome of an enslaved algal nucleus.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11323671},
volume = {410},
