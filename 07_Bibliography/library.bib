@article{Paule2000,
abstract = {The task of transcribing nuclear genes is shared between three RNA polymerases in eukaryotes: RNA polymerase (pol) I synthesizes the large rRNA, pol II synthesizes mRNA and pol III synthesizes tRNA and 5S rRNA. Although pol II has received most attention, pol I and pol III are together responsible for the bulk of transcriptional activity. This survey will summarise what is known about the process of transcription by pol I and pol III, how it happens and the proteins involved. Attention will be drawn to the similarities between the three nuclear RNA polymerase systems and also to their differences.},
