journal = {Cardiovascular Intervention and Therapeutics},
keywords = {Fractional flow reserve,Instantaneous wave-free ratio,Resting indexes},
number = {0123456789},
publisher = {Springer Japan},
title = {{In vivo validation of resting full-cycle ratio and diastolic pressure ratio: simultaneous measurement with instantaneous wave-free ratio}},
url = {https://doi.org/10.1007/s12928-020-00648-4},
