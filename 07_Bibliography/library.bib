pages = {2258--2265},
title = {{GenoGAM: genome-wide generalized additive models for ChIP-Seq analysis}},
type = {Journal Article},
url = {https://watermark.silverchair.com/btx150.pdf?token=AQECAHi208BE49Ooan9kkhW{\_}Ercy7Dm3ZL{\_}9Cf3qfKAc485ysgAAAnMwggJvBgkqhkiG9w0BBwagggJgMIICXAIBADCCAlUGCSqGSIb3DQEHATAeBglghkgBZQMEAS4wEQQMgWeMEJHZvaQhhTNVAgEQgIICJi73l2KqHbwaJk9jACrrbx4VrQIgFzysBe3ERlnNWkC8RMtR},
volume = {33},
