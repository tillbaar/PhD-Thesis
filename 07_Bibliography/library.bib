issn = {14337851},
journal = {Angewandte Chemie International Edition},
keywords = {LC-MS,RNA modifications,isotope labelling,radical-SAM enzymes,thioacetals},
month = {jun},
number = {26},
pages = {7893--7897},
pmid = {29624844},
title = {{A Vastly Increased Chemical Variety of RNA Modifications Containing a Thioacetal Structure}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/29624844 http://doi.wiley.com/10.1002/anie.201713188},
volume = {57},
year = {2018}
}
