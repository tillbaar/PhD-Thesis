journal = {Nature},
keywords = {Artifacts,DNA Replication Timing,Exome,Exome: genetics,False Positive Reactions,Gene Expression,Genetic Heterogeneity,Genome, Human,Genome, Human: genetics,Humans,Lung Neoplasms,Lung Neoplasms: genetics,Mutation,Mutation Rate,Mutation: genetics,Neoplasms,Neoplasms, Squamous Cell,Neoplasms, Squamous Cell: genetics,Neoplasms: classification,Neoplasms: genetics,Neoplasms: pathology,Oncogenes,Oncogenes: genetics,Reproducibility of Results,Sample Size},
number = {7457},
pages = {214--8},
pmid = {23770567},
title = {{Mutational heterogeneity in cancer and the search for new cancer-associated genes.}},
