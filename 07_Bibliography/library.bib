keywords = {08923973,1,10,2012,2013,2013 informa healthcare usa,3109,35,7,710636,doi,inc,issn 0892-3973 print,issn 1532-2513 online,unopharmacology and immunotoxicology},
month = {feb},
number = {1},
pages = {1--7},
pmid = {22856509},
title = {{Antiviral activity of acidic polysaccharides from Coccomyxa gloeobotrydiformi, a green alga, against an in vitro human influenza A virus infection.}},
