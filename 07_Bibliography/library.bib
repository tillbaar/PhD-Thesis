keywords = {Cell Line,Chlamydomonas,Chlamydomonas: metabolism,Humans,Hydrogen-Ion Concentration,Kinetics,Rhodopsin,Rhodopsin: metabolism},
number = {1},
pages = {119--28},
pmid = {22044280},
title = {{Diversity of Chlamydomonas channelrhodopsins.}},
