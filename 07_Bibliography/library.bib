isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Gene Expression Regulation,Gene Regulatory Networks,Gene Regulatory Networks: genetics,Genetic Engineering,Genetic Engineering: methods,RNA,RNA, Antisense,RNA, Antisense: genetics,RNA: genetics,Signal Transduction,Signal Transduction: genetics,Synthetic Biology,Synthetic Biology: methods,Transcription Factors,Transcription, Genetic},
number = {21},
pages = {8617--22},
pmid = {21555549},
title = {{Versatile RNA-sensing transcriptional regulators for engineering genetic networks.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3102349{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {108},
year = {2011}
}
@article{Chen2015,
abstract = {We developed Copy Number Segmentation by Regression Tree in Next Generation Sequencing (CONSERTING), an algorithm for detecting somatic copy-number alteration (CNA) using whole-genome sequencing (WGS) data. CONSERTING performs iterative analysis of segmentation on the basis of changes in read depth and the detection of localized structural variations, with high accuracy and sensitivity. Analysis of 43 cancer genomes from both pediatric and adult patients revealed novel oncogenic CNAs, complex rearrangements and subclonal CNAs missed by alternative approaches.},
