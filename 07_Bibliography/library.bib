issn = {19326203},
journal = {PLoS ONE},
keywords = {Chromatin,DNA transcription,Enhancer elements,Epigenomics,Genome annotation,Genomics,Morphogenic segmentation,Transcription factors},
month = {jan},
number = {1},
pages = {e0169249},
pmid = {28056037},
publisher = {Public Library of Science},
title = {{Accurate promoter and enhancer identification in 127 ENCODE and roadmap epigenomics cell types and tissues by GenoSTAN}},
url = {http://bioconductor.org/},
