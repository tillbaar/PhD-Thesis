issn = {22111247},
journal = {Cell Reports},
month = {dec},
number = {12},
pages = {2653--2662},
pmid = {26711333},
publisher = {Elsevier},
title = {{Nuclear Retention of mRNA in Mammalian Tissues}},
url = {http://www.cell.com/article/S2211124715013510/fulltext http://www.cell.com/article/S2211124715013510/abstract https://www.cell.com/cell-reports/abstract/S2211-1247(15)01351-0},
volume = {13},
