pmid = {21510875},
publisher = {BioMed Central Ltd},
title = {{Algal MIPs, high diversity and conserved motifs.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3111385{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {11},
year = {2011}
}
