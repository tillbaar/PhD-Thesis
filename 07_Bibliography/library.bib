journal = {Journal of the American Society of Nephrology},
month = {apr},
number = {4},
pages = {533--545},
publisher = {American Society of Nephrology},
title = {{Single-cell RNA profiling of glomerular cells shows dynamic changes in experimental diabetic kidney disease}},
volume = {30},
year = {2019}
}
