year = {2015}
}
@incollection{Tisdale2017,
abstract = {Spinal muscular atrophy (SMA)-the leading genetic cause of death in infancy characterized by degeneration of motor neurons and atrophy of skeletal muscle-is a prominent example of the increasing number of neurological diseases that are caused by mutations in ubiquitously expressed genes implicated in RNA regulation. SMA is caused by a ubiquitous deficiency in the survival motor neuron (SMN) protein-the key component of a macromolecular machine essential for the assembly of diverse RNA-protein complexes that participate in various aspects of RNA processing and posttranscriptional gene regulation. The identification of the full spectrum of RNA-processing pathways directly controlled by SMN and dissection of the downstream consequences of SMN deficiency on RNA regulation that impact motor system function are fundamental steps toward understanding disease mechanisms. This chapter discusses how advances in our understanding of the functions of SMN and the effects of RNA-processing dysregulation induced by SMN deficiency are helping to unravel the molecular basis of SMA.},
author = {Tisdale, S. and Pellizzoni, L.},
booktitle = {Spinal Muscular Atrophy: Disease Mechanisms and Therapy},
doi = {10.1016/B978-0-12-803685-3.00007-0},
