keywords = {Animals,Fetal Proteins,Fetal Proteins: genetics,Fetal Proteins: metabolism,Gene Expression Regulation, Developmental,Hydrozoa,Hydrozoa: anatomy \& histology,Hydrozoa: embryology,Hydrozoa: physiology,Metamorphosis, Biological,Regeneration,Signal Transduction,T-Box Domain Proteins,T-Box Domain Proteins: genetics,T-Box Domain Proteins: metabolism,TCF Transcription Factors,TCF Transcription Factors: genetics,TCF Transcription Factors: metabolism,Wnt Proteins,Wnt Proteins: genetics,Wnt Proteins: metabolism,beta Catenin},
month = sep,
