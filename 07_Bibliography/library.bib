journal = {Antimicrobial agents and chemotherapy},
keywords = {Anti-Infective Agents,Bacterial,Base Sequence,Computational Biology,Databases,Drug Resistance,Genes,Genetic,Genome,Internet,Microbial,Microbial: genetics,User-Computer Interface},
month = {jul},
