journal = {Nature Methods},
keywords = {Gene expression analysis,RNA metabolism,RNA sequencing},
month = {dec},
number = {12},
pages = {1198--1204},
publisher = {Nature Publishing Group},
title = {{Thiol-linked alkylation of RNA to assess expression dynamics}},
url = {http://www.nature.com/articles/nmeth.4435},
volume = {14},
year = {2017}
}
