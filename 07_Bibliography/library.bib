doi = {10.1093/bioinformatics/btv566},
file = {:C$\backslash$:/Users/Admin/Documents/Papers/2016/2016 - Okonechnikov, Conesa, Garc{\'{i}}a-Alcalde.pdf:pdf},
issn = {1367-4811},
