@article{Plagens2015,
abstract = {Signal recognition particles (SRPs) are universal ribonucleoprotein complexes found in all three domains of life that direct the cellular traffic and secretion of proteins. These complexes consist of SRP proteins and a single, highly structured SRP RNA. Canonical SRP RNA genes have not been identified for some Thermoproteus species even though they contain SRP19 and SRP54 proteins. Here, we show that genome rearrangement events in Thermoproteus tenax created a permuted SRP RNA gene. The 5'- and 3'-termini of this SRP RNA are located close to a functionally important loop present in all known SRP RNAs. RNA-Seq analyses revealed that these termini are ligated together to generate circular SRP RNA molecules that can bind to SRP19 and SRP54. The circularization site is processed by the tRNA splicing endonuclease. This moonlighting activity of the tRNA splicing machinery permits the permutation of the SRP RNA and creates highly stable and functional circular RNA molecules.},
author = {Plagens, Andr{\'{e}} and Daume, Michael and Wiegel, Julia and Randau, Lennart},
doi = {10.7554/eLife.11623.001},
file = {:C$\backslash$:/Users/Admin/Documents/Papers/2015/2015 - Plagens et al.pdf:pdf},
