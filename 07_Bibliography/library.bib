isbn = {1466-5026},
issn = {14665026},
journal = {International Journal of Systematic and Evolutionary Microbiology},
keywords = {0,02859-0,1099,2004,54,975,986,and evolutionary microbiology,basidiomycetous,doi 10,ernational journal of systematic,ijs,systematics of the anamorphic,with the,yeast genus trichosporon behrend},
