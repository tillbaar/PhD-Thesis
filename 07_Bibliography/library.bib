number = {1528},
pages = {501--38},
pmid = {11700292},
title = {{Biology of mammalian L1 retrotransposons.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11700292},
volume = {35},
year = {2001}
}
@article{Marin2003,
abstract = {Sequence comparisons and a revised classification of the Euglenophyceae were based on 92 new SSU rDNA sequences obtained from strains of Euglena, Astasia, Phacus, Trachelomonas, Colacium, Cryptoglena, Lepocinclis, Eutreptia, Eutreptiella and Tetreutreptia. Sequence data also provided molecular signatures for taxa from genus to class level in the SSU rRNA secondary structure, revealed by a novel approach (search for non-homoplasious synapomorphies) and used for taxonomic diagnoses. Photosynthetic euglenoids and secondary heterotrophs formed a clade, designated as Euglenophyceae (emend.) with two orders: Euglenales and Eutreptiales. The mostly marine Eutreptiales (Eutreptia, Eutreptiella; not Distigma) comprised taxa with two or four emergent flagella (the quadriflagellate Tetreutreptia was integrated within Eutreptiella). The Euglenales (freshwater genera with {\textless} or = one emergent flagellum) formed nine clades and two individual branches (single strains); however, only two clades were congruent with traditional genera: Trachelomonas (incl. Strombomonas) and Colacium. Euglena was polyphyletic and diverged into four independent clades (intermixed with Astasia, Khawkinea and Lepocinclis) and two individual branches (e.g. E. polymorpha). Phacus was also subdivided into Phacus s. str. and two combined lineages (mixed with Lepocinclis spp. or Cryptoglena). In consequence, Euglena (s. str.), Phacus and other genera were emended and one lineage (mixed Phacus/Lepocinclis-clade) was recognized as the previously neglected genus Monomorphina Mereschkowsky (1877). The sister clade of Phacus s. str. (mixed Euglena/Lepocinclis-clade) was identified as Lepocinclis Perty (emended).},
author = {Marin, Birger and Palm, Anne and Klingberg, Max and Melkonian, Michael},
file = {:C$\backslash$:/Users/Admin/Documents/Papers/2003/2003 - Marin et al.pdf:pdf},
issn = {1434-4610},
journal = {Protist},
keywords = {Animals,Base Sequence,Classification,DNA Primers,Euglena,Euglena: classification,Euglena: genetics,Euglenida,Euglenida: classification,Euglenida: genetics,Models, Molecular,Molecular Sequence Data,Nucleic Acid Conformation,Phylogeny,Plastids,Plastids: genetics,RNA, Plant,RNA, Plant: chemistry,RNA, Plant: genetics,RNA, Protozoan,RNA, Protozoan: chemistry,RNA, Protozoan: genetics,RNA, Ribosomal,RNA, Ribosomal: chemistry,RNA, Ribosomal: genetics,Species Specificity},
month = {apr},
number = {1},
pages = {99--145},
pmid = {12812373},
title = {{Phylogeny and taxonomic revision of plastid-containing euglenophytes based on SSU rDNA sequence comparisons and synapomorphic signatures in the SSU rRNA secondary structure.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12812373},
volume = {154},
