arxivId = {arXiv:1011.1669v3},
author = {Caisov{\'{a}}, Lenka and Marin, Birger and Sausen, Nicole and Pr{\"{o}}schold, Thomas and Melkonian, Michael},
doi = {10.1111/j.1529-8817.2010.00949.x},
eprint = {arXiv:1011.1669v3},
file = {:C$\backslash$:/Users/Admin/Documents/Papers/2011/2011 - Caisov{\'{a}} et al.pdf:pdf},
isbn = {9788578110796},
issn = {00223646},
journal = {Journal of Phycology},
keywords = {18S rDNA,Aphanochaete,Caespitella,Chaetophora,Chaetophorales,Draparnaldia,Fritschiella,Phylogeny,Polyphyly,Stigeoclonium},
month = {feb},
