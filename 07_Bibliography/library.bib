title = {{ERK MAP kinase-activated Arf6 trafficking directs coxsackievirus type B3 into an unproductive compartment during virus host-cell entry.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19264606},
volume = {90},
year = {2009}
}
