isbn = {1367-4803},
issn = {13674803},
journal = {Bioinformatics},
number = {6},
pages = {813--817},
pmid = {14751968},
title = {{Alu repeat analysis in the complete human genome: Trends and variations with respect to genomic composition}},
volume = {20},
year = {2004}
}
