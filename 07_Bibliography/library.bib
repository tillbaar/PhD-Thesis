keywords = {Algorithms,Binding Sites,DNA,DNA Primers,DNA Primers: chemistry,DNA: chemistry,Internet,Polymerase Chain Reaction,Sequence Analysis, DNA,Software,Templates, Genetic,Thermodynamics},
month = {jul},
number = {Web Server issue},
pages = {W205--8},
pmid = {22689644},
title = {{MFEprimer-2.0: a fast thermodynamics-based program for checking PCR primer specificity.}},
