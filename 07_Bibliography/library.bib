issn = {00951137},
journal = {Journal of Clinical Microbiology},
keywords = {00 ϩ 0 doi,0095-1137,04,08,10,1128,12,2004,42,5467,5471,all rights reserved,american society for microbiology,copyright,dec,jcm,m icrobiology,p,urnal of c linical},
