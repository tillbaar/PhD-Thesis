keywords = {MRNA transport,Motor neuron disease,Neurodegeneration,RNA processing,Ribonucleoprotein complexes (RNPs),Sm and LSm proteins,Small nuclear RNAs (snRNAs),Spinal muscular atrophy (SMA),Splicing,Survival motor neuron (SMN)},
