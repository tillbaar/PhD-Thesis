@article{Lynch2011,
abstract = {The gene regulatory network (GRN) underpinning dorsal-ventral (DV) patterning of the Drosophila embryo is among the most thoroughly understood GRNs, making it an ideal system for comparative studies seeking to understand the evolution of development. With the emergence of widely applicable techniques for testing gene function, species with sequenced genomes, and multiple tractable species with diverse developmental modes, a phylogenetically broad and molecularly deep understanding of the evolution of DV axis formation in insects is feasible. Here, we review recent progress made in this field, compare our emerging molecular understanding to classical embryological experiments, and suggest future directions of inquiry.},
