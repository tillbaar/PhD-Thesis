journal = {Nucleic Acids Res.},
keywords = {Animals,Base Sequence,Cells,Cloning,Cultured,Cytoplasm/analysis,DNA-Directed RNA Polymerases/metabolism,DNA/analysis,Genetic,Human,Hybrid Cells/analysis,Mice,Molecular,Nucleic Acid,RNA Polymerase III/metabolism,Repetitive Sequences,Templates,Transcription},
number = {5},
pages = {1171--1189},
pmid = {7232214},
title = {{Transcriptional analysis of interspersed repetitive polymerase III transcription units in human DNA}},
volume = {9},
year = {1981}
}
