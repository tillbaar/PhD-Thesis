pages = {1002236},
pmid = {21876680},
publisher = {Public Library of Science},
title = {{A comprehensive map of mobile element insertion polymorphisms in humans}},
url = {https://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1002236},
volume = {7},
year = {2011}
